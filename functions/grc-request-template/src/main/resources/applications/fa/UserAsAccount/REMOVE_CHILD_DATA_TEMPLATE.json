{
  "id": "1",
  "name": "User Remove Role membership",
  "method": "PATCH",
  "uri": {
    "scheme": "<<SCHEME>>",
    "host": "<<HOST>>",
    "path": "/hcmRestApi/scim/Roles/<EL>attributes.get('roles').get('uid').get(0)</EL>"
  },
  "headers": [
    {
      "name": "Content-Type",
      "value": "application/json"
    },
    {
      "name": "Authorization",
      "value": "<<CREDENTIALS>>"
    }
  ],
  "body": {
    "type": "text",
    "textBody": {
      "members": [
        {
          "operation": "REMOVE",
          "value": "<EL>attributes.get('uid').get(0)</EL>"
        }
      ]
    }
  }
}